  },
  "homepage": "https://github.com/HiromiShikata/profile#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^9.1.1",
    "jsonresume-theme-paper": "x.x.x",
    "lodash": "^4.17.15",
    "mustache": "^4.0.1",
    "prettier": "^2.0.5",
    "resume-cli": "x.x.x",
    "standard-version": "^8.0.0",
    "underscore.string": "^3.3.5"
  },
  "dependencies": {
    "ansi-styles": "^4.2.1",
